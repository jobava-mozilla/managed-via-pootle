msgid "Sent:"
msgstr "寄出於:"

#: mozillians/templates/phonebook/invite.html:66
msgid "Redeemed:"
msgstr "兌換於:"

#: mozillians/templates/phonebook/invite.html:69
msgid "Redeemer:"
msgstr "兌換者:"

